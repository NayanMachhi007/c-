@model IEnumerable<practice_database.Data.studentInfo>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "_Layout" ; 
}

<table class="table">
    <thead>
        <tr>
            <th>roll number</th>
            <th>Name</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.RollNo</td>
                <td>@item.StudentName</td>
                <td>@item.password</td>
            </tr>
        }
        
    </tbody>
</table>